{
  "name": "humanize-duration",
  "author": "Evan Hahn <me@evanhahn.com> (http://evanhahn.com)",
  "contributors": [
    "Martin Prins (https://github.com/magarcia)",
    "Filipi Siqueira (https://github.com/filipi777)",
    "Peter Rekdal Sunde (https://github.com/peters)",
    "Micha≈Ç Janiec (https://github.com/mjjaniec)"
  ],
  "version": "0.7.0",
  "description": "Convert millisecond durations to English or many other languages.",
  "main": "humanize-duration.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha"
  },
  "devDependencies": {
    "mocha": "1.18.x",
    "chai": "1.9.x",
    "sugar": "1.4.x",
    "coffee-script": "1.7.x"
  },
  "keywords": [
    "humanize",
    "duration",
    "time",
    "hours",
    "minutes",
    "seconds",
    "days",
    "years",
    "months",
    "english",
    "spanish",
    "french",
    "catalan",
    "portuguese",
    "korean",
    "norwegian",
    "german"
  ],
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "curly": false,
    "eqeqeq": false,
    "es3": true,
    "freeze": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": true,
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "predef": [
      "module",
      "require",
      "describe",
      "it",
      "beforeEach"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/EvanHahn/HumanizeDuration.js.git"
  },
  "license": "WTFPL"
}
