{
  "name": "humanize-duration-demo",
  "version": "0.0.0",
  "private": true,
  "description": "interactive demo for EvanHahn/HumanizeDuration.js",
  "dependencies": {
    "highlight.js": "^8.7.0",
    "humanize-duration": "3.5.0",
    "random-number": "0.0.6",
    "react": "^0.13.3"
  },
  "scripts": {
    "build": "cp src/*.html dev/ & npm run bundle",
    "bundle": "webpack --progress --colors",
    "serve": "webpack-dev-server --devtool inline-source-map --content-base dev/",
    "gh": "npm run clean && npm run build && npm run gh-js && npm run gh-css && npm run gh-static",
    "gh-js": "uglifyjs --compress --mangle -- dev/bundle.js > ./bundle.js",
    "gh-css": "cleancss dev/style.css > ./style.css",
    "gh-static": "cp dev/*.html ./",
    "clean": "rm -f dev/* & rm -f ./*.html & rm -f ./style.css & rm -f ./bundle.js"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel-core": "^5.7.4",
    "babel-loader": "^5.3.2",
    "clean-css": "^3.3.7",
    "css-loader": "^0.15.5",
    "extract-text-webpack-plugin": "^0.8.2",
    "node-libs-browser": "^0.5.2",
    "postcss-loader": "^0.5.1",
    "style-loader": "^0.12.3",
    "uglify-js": "^2.4.24",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  }
}
